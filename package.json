{
  "private": true,
  "workspaces": [
    "packages/*",
    "commander"
  ],
  "devDependencies": {
    "@astral-frontend/eslint-config-ts-ui": "2.0.1",
    "@astral/commander": "*",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-eslint": "^10.1.0",
    "commitlint": "16.2.1",
    "eslint": "^7.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.1.2",
    "read-dir-names": "1.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4"
  },
  "scripts": {
    "lint": "npm run lint --workspaces --if-present",
    "lint:types": "npm run lint:types --workspaces --if-present",
    "publish": "npm run build --workspaces --if-present && npm run publish --workspaces --if-present",
    "tests": "npm run test --workspaces --if-present"
  }
}
